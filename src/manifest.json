{

  "manifest_version": 2,
  "name": "NQuiz",
  "version": "0.0.1",

  "description": "turns news titles into quiz items",

  "icons": {
    "48": "border-48.png"
  },

  "content_scripts": [
    {
      "matches": ["https://www.nrk.no/*"],
      "run_at" : "document_idle",
      "js": ["inject.js"]
    }
  ]

}